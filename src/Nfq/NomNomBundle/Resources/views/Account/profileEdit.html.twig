{% extends "NfqNomNomBundle:Default:index.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block content %}
    <div class="EditBox">
        <form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST" class="formBox">
            {{ form_widget(form) }}
            <div style="padding: 5px">
                <input class="profileButton" type="submit" value="{{ 'profile.edit.submit'|trans }}"/>
            </div>
        </form>
    </div>
{% endblock %}